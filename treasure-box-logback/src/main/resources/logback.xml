<!-- json格式 -->
<configuration>
    <appender name="json" class="ch.qos.logback.core.ConsoleAppender">
        <encoder name="LogstashEncoder" class="net.logstash.logback.encoder.LogstashEncoder">
        </encoder>
    </appender>
    <root level="DEBUG">
        <appender-ref ref="json" />
    </root>
</configuration>

